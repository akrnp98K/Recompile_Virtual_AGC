C/ From PDF p. 159 of "Programming in HAL/S".

  AGE:
  PROGRAM;
     DECLARE C CHARACTER(80);
     DECLARE INTEGER,
     		CASE_NUM, AGE, SEX, X;
E               ,
M    READALL(5) C;
E			,
M    CASE_NUM = INTEGER(C      );
S			 1 TO 3

E		   ,
M    AGE = INTEGER(C      );
S		    4 TO 5

E		   ,
M    SEX = INTEGER(C );
S		    6

E		 ,
M    X = INTEGER(C       );
S		  7 TO 10

  CLOSE AGE;
  